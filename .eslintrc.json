{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery"
	],
	"globals": {
		"OO": false,
		"mw": false
	},
	"rules": {
		"eqeqeq": "warn",
		"no-undef": "warn",
		"camelcase": "warn",
		"no-implicit-globals": "warn",
		"no-alert": "warn",
		"no-unused-vars": "warn",
		"no-jquery/no-animate-toggle": "warn",
		"no-jquery/no-global-selector": "warn"
	}
}
